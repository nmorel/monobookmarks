{
  "name": "@nmorel/monobookmarks",
  "version": "1.0.0",
  "repository": "https://github.com/nmorel/monobookmarks.git",
  "author": "Nicolas Morel <nmr.morel@gmail.com>",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "config/*",
      "dev/*",
      "libs/*"
    ],
    "nohoist": [
      "**/module-alias"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "turbo": "^1.2.16"
  },
  "scripts": {
    "build": "turbo run build",
    "ci": "yarn install --frozen-lockfile",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "tscheck": "turbo run tscheck"
  }
}
